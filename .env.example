# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1  # Default OpenAI API endpoint

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pixels_db
DB_USER=postgres
DB_PASSWORD=your_db_password_here

# Model Configuration
DEFAULT_MODEL=gpt-4-turbo-preview  # or gpt-3.5-turbo for testing
TEMPERATURE=0.7
MAX_TOKENS=2000

# Embedding Configuration
EMBEDDING_MODEL=BAAI/bge-small-en-v1.5  # Fast, efficient embedding model
EMBEDDING_DEVICE=cpu  # or cuda if GPU is available

# Agent Configuration
WORK_DIR=./workspace  # Directory for agent file operations
USE_DOCKER=false      # Set to true to run code in docker 